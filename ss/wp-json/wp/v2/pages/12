do para 100ms\n\u00a0\u00a0\u00a0\u00a0};\n<\/script>\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t<div class=\"elementor-element elementor-element-164dfe0e e-con-full e-flex e-con e-parent\" data-id=\"164dfe0e\" data-element_type=\"container\" data-settings=\"{&quot;content_width&quot;:&quot;full&quot;,&quot;background_background&quot;:&quot;classic&quot;}\" data-core-v316-plus=\"true\">\n\t\t\t\t<div class=\"elementor-element elementor-element-19bb19aa elementor-widget__width-inherit elementor-widget elementor-widget-html\" data-id=\"19bb19aa\" data-element_type=\"widget\" data-settings=\"{&quot;_animation_mobile&quot;:&quot;slideInDown&quot;}\" data-widget_type=\"html.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t<div class=\"checkout-container\">\r\n    <iframe class=\"checkout-iframe\" src=\"https:\/\/pepper.com.br\/checkout\/index.html?p=78926&o=115431\" onload=\"handleIframeLoad()\" width=\"600\" height=\"1850\" scrolling=\"no\"><\/iframe>\r\n<\/div>\r\n\r\n<script>\r\nvar loadCount = 0; \/\/ Contador para acompanhar o n\u00famero de vezes que o iframe foi carregado\r\nvar utmUpdated = false; \/\/ Flag para verificar se os par\u00e2metros UTM foram adicionados\r\n\r\nfunction getUTMParameters() {\r\n    var utmParameters = {};\r\n    var queryString = window.location.search.substring(1);\r\n    var queryParams = queryString.split('&');\r\n\r\n    for (var i = 0; i < queryParams.length; i++) {\r\n        var pair = queryParams[i].split('=');\r\n        if (pair[0].startsWith('utm_')) {\r\n            utmParameters[pair[0]] = pair[1];\r\n        }\r\n    }\r\n\r\n    return utmParameters;\r\n}\r\n\r\nfunction appendUTMParametersToIframeSrc() {\r\n    var iframe = document.querySelector('.checkout-iframe');\r\n    var originalSrc = iframe.getAttribute('src');\r\n    var utmParameters = getUTMParameters();\r\n    var defaultUTM = {\r\n        'utm_source': 'Facebook',\r\n        'utm_medium': 'Conjunto',\r\n        'utm_campaign': 'Campanha',\r\n        'utm_content': 'Criativo'\r\n    };\r\n\r\n    \/\/ Sobrescreve os padr\u00f5es com os valores da URL, se dispon\u00edveis\r\n    for (var key in defaultUTM) {\r\n        if (utmParameters[key]) {\r\n            defaultUTM[key] = utmParameters[key];\r\n        }\r\n    }\r\n\r\n    var utmQueryString = Object.keys(defaultUTM)\r\n                             .map(key => key + '=' + defaultUTM[key])\r\n                             .join('&');\r\n    var newSrc = originalSrc.includes('?') ? originalSrc + '&' + utmQueryString : originalSrc + '?' + utmQueryString;\r\n\r\n    iframe.setAttribute('src', newSrc);\r\n    utmUpdated = true; \/\/ Marca que os par\u00e2metros UTM foram atualizados\r\n}\r\n\r\nfunction handleIframeLoad() {\r\n    loadCount++; \/\/ Incrementa a contagem de carregamento\r\n    var iframe = document.querySelector('.checkout-iframe');\r\n    var container = document.querySelector('.checkout-container');\r\n\r\n    if (loadCount === 1) {\r\n        \/\/ Primeiro carregamento - exibe o iframe e adiciona UTM\r\n        iframe.style.display = 'block';\r\n        appendUTMParametersToIframeSrc();\r\n    } else if (loadCount === 2 && utmUpdated) {\r\n        \/\/ Carregamento ap\u00f3s a atualiza\u00e7\u00e3o UTM - n\u00e3o faz altera\u00e7\u00f5es visuais\r\n        console.log(\"UTM Parameters added at \" + new Date().toLocaleTimeString());\r\n    } else if (loadCount === 3 || (loadCount === 2 && !utmUpdated)) {\r\n        \/\/ Carregamento para a p\u00e1gina de pagamento - ajusta o CSS\r\n        container.style.height = '800px';\r\n        iframe.style.height = '800px';\r\n        iframe.style.top = '-20px';\r\n        iframe.style.display = 'block'; \/\/ Certifique-se de que o iframe esteja vis\u00edvel\r\n        console.log(\"Payment page loaded at \" + new Date().toLocaleTimeString());\r\n    }\r\n}\r\n\r\n<\/script>\r\n\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t\t\t<\/div>\n\t\t","protected":fa